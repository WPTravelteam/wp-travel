{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wptravelblock/trip-list",
  "version": "0.1.0",
  "title": "Trip List",
  "category": "wp-travel-blocks",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "attributes": {
    "query": {
      "type": "object",
      "default": {
        "numberOfItems": 3,
        "orderBy": "title",
        "order": "asc"
      }
    },
    "saleTrip": {
      "type": "boolean",
      "default": false
    },
    "featuredTrip": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "trip-list",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}